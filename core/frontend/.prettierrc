{
	"arrowParens": "always",
	"endOfLine": "lf",
	"semi": true,
	"singleQuote": true,
	"alignObjectProperties": true,
	"quoteProps": "as-needed",
	"trailingComma": "all",
	"useTabs": true,
	"jsxSingleQuote": true,
	"tabWidth": 4,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
